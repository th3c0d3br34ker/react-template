{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^2.1.2"
  },
  "engines": {
    "npm": ">=8.1.1",
    "node": ">=16.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write src/**/*.js"
    ],
    "*.css": [
      "prettier --write src/**/*.css"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.1.0",
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3"
  }
}
